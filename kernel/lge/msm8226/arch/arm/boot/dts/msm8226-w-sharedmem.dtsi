/ {
	rmtfs_sharedmem {
		compatible = "qcom,sharedmem-uio";
		reg = <0x07f00000 0x00100000>;
		reg-names = "rmtfs";
	};

	dsp_sharedmem {
		compatible = "qcom,sharedmem-uio";
		reg = <0x03260000 0x00020000>;
		reg-names = "rfsa_dsp";
	};

	mdm_sharedmem {
		compatible = "qcom,sharedmem-uio";
		reg = <0x03260000 0x00020000>;
		reg-names = "rfsa_mdm";
	};
};
